{
  "langrev": "1",
  "warnings": [ "no-unused" ],
  "dependency-search-paths": [ "lib" ],
  "dependencies": [ "playdate" ],
  "authors": [ "lilah <lilah.willacott@gmail.com>" ],
  "version": "0.1.0",
  "sources": [ "src/**" ],
  "output": "build",

  "targets": {
    "playdate_windows64": {
      "target": "windows-x64",
      "type": "dynamic-lib",
    },
    "playdate_linux64": {
      "target": "linux-x64",
      "type": "dynamic-lib"
    },
    "playdate_device": {
      "type": "executable",
      "target": "elf-aarch64",
      "cpu": "cortex-m7",
      "memory-env": "tiny",
    },
  },
  "cpu": "generic",
  "opt": "O0"
}
